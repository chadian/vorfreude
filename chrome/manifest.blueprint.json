{
  "name": "Vorfreude",
  "description": "When you can't wait but have to",
  "version": "{{packageVersion}}",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "vorfreude",
    "default_icon": {
      "32": "images/icon32.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "content_security_policy": "{{ContentSecurityPolicy}}",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ]
}
