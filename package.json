{
  "name": "svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:extension": "yarn build && cd chrome && yarn build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "yarn test:unit && yarn test:acceptance",
    "test:acceptance": "playwright test tests/acceptance",
    "test:unit": "jest src",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.25.1",
    "@sveltejs/adapter-auto": "1.0.0-next.64",
    "@sveltejs/adapter-static": "^1.0.0-next.41",
    "@sveltejs/kit": "1.0.0-next.417",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "^0.0.196",
    "@types/luxon": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^28.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "luxon": "^2.4.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "ramda": "^0.28.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.7.1",
    "svelte-htm": "^1.1.1",
    "svelte-inline-compile": "^0.2.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.8.0",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4",
    "vite": "^3.0.0"
  },
  "type": "module"
}
