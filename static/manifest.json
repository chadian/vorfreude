{
  "name": "Vorfreude",
  "description": "When you can't wait but have to",
  "version": "0.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "vorfreude",
    "default_icon": {
      "32": "images/icon32.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ]
}
